<script>\
\
var counter=0;\
var totalQuestions=4;\
\
var myGraduationYear= 2026;\
var myMajor="English";\
\
//create a list of the majors you are/ were interested in\
var majorInterestedList=["biology", "philosophy", "environmental studies", "economics" ];\
\
var favArtistsList=["Lake Street Dive", "Ariana Grande", "Billie Holiday"];\
\
var userName;\
\
    function displayFeedback(form) \{\
\
        counter++;\
\
        var feedbackText="";\
        var newQuestionText="";\
\
        var userInput= form.userInput.value;\
\
        if (counter==1) \{        \
            userName= userInput;\
            feedbackText="Hello, "+ userName+" ! " +"<br>Nice to meet you."+\
