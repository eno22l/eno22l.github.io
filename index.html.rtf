\
        if (counter==6) \{     \
            //check if user's favorite artist is one of the artists in your favArtistsList \
            for (let i = 0; i < favArtistsList.length; i++) \{      \
                if (userInput.toLowerCase() == favArtistsList[i].toLowerCase()) \{\
                    feedbackText=userInput+ " That\'92s one of my favorites too!";\
                \}    \
            \}\
            //if you have reached the last question\
            if (counter>=totalQuestions ) \{\
                feedbackText=feedbackText+"<br><br>It was nice talking to you, "+userName+"."+\
